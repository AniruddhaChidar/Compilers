Grammar

    0 $accept: PROGRAM $end

    1 PROGRAM: GLOBAL_DECLARATIONS MAIN_FUNCTION

    2 GLOBAL_DECLARATIONS: %empty
    3                    | GLOBAL_DECLARATIONS DECLARATION

    4 DECLARATION: TYPE NAME_LIST ';'

    5 NAME_LIST: NAME
    6          | NAME ',' NAME_LIST

    7 TYPE: INTEGER
    8     | FLOAT

    9 DEF: VOID NAME

   10 MAIN_FUNCTION: DEF '(' ')' '{' PROCEDURE '}'

   11 PROCEDURE: LOCAL_DECLARATIONS STATEMENT_LIST

   12 LOCAL_DECLARATIONS: %empty
   13                   | LOCAL_DECLARATIONS DECLARATION

   14 STATEMENT_LIST: %empty
   15               | STATEMENT_LIST STATEMENT

   16 STATEMENT: ASSIGN_STATEMENT
   17          | IF_STATEMENT
   18          | IF_ELSE_STATEMENT
   19          | WHILE_STATEMENT
   20          | DO_WHILE_STATEMENT

   21 DO_WHILE_STATEMENT: DO '{' STATEMENT_LIST '}' WHILE '(' COND_EXP ')' ';'

   22 WHILE_STATEMENT: WHILE '(' COND_EXP ')' '{' STATEMENT_LIST '}'
   23                | WHILE '(' COND_EXP ')' STATEMENT

   24 IF_STATEMENT: IF '(' COND_EXP ')' '{' STATEMENT_LIST '}'
   25             | IF '(' COND_EXP ')' STATEMENT

   26 BALANCED_IF_STATEMENT: ASSIGN_STATEMENT
   27                      | DO_WHILE_STATEMENT
   28                      | WHILE '(' COND_EXP ')' '{' STATEMENT_LIST '}'
   29                      | WHILE '(' COND_EXP ')' BALANCED_IF_STATEMENT
   30                      | IF '(' COND_EXP ')' '{' STATEMENT_LIST '}' ELSE '{' STATEMENT_LIST '}'
   31                      | IF '(' COND_EXP ')' BALANCED_IF_STATEMENT ELSE '{' STATEMENT_LIST '}'
   32                      | IF '(' COND_EXP ')' BALANCED_IF_STATEMENT ELSE BALANCED_IF_STATEMENT
   33                      | IF '(' COND_EXP ')' '{' STATEMENT_LIST '}' ELSE BALANCED_IF_STATEMENT

   34 IF_ELSE_STATEMENT: IF '(' COND_EXP ')' '{' STATEMENT_LIST '}' ELSE '{' STATEMENT_LIST '}'
   35                  | IF '(' COND_EXP ')' BALANCED_IF_STATEMENT ELSE '{' STATEMENT_LIST '}'
   36                  | IF '(' COND_EXP ')' BALANCED_IF_STATEMENT ELSE STATEMENT
   37                  | IF '(' COND_EXP ')' '{' STATEMENT_LIST '}' ELSE STATEMENT

   38 ASSIGN_STATEMENT: NAME '=' ARITH_EXP ';'

   39 COND_EXP: ARITH_EXP RELOP ARITH_EXP
   40         | ARITH_EXP EQOP ARITH_EXP
   41         | COND_EXP AND COND_EXP
   42         | COND_EXP OR COND_EXP
   43         | NOT COND_EXP
   44         | COND_EXP '?' COND_EXP ':' COND_EXP
   45         | '(' COND_EXP ')'

   46 ARITH_EXP: INTEGER_NUMBER
   47          | NAME
   48          | DOUBLE_NUMBER
   49          | ARITH_EXP '+' ARITH_EXP
   50          | ARITH_EXP '*' ARITH_EXP
   51          | ARITH_EXP '-' ARITH_EXP
   52          | ARITH_EXP '/' ARITH_EXP
   53          | '-' ARITH_EXP
   54          | '(' ARITH_EXP ')'
   55          | COND_EXP '?' ARITH_EXP ':' ARITH_EXP


